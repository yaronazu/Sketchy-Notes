{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "commands": [
    {
      "name": "Sketchy Notes",
      "shortcut": "cmd option b",
      "script": "sketchynotes.js",
      "handlers": {
        "run": "default",
        "actions": {
          "OpenDocument": "onOpenDocument",
          "InsertSymbol": "SymbolAdded",
          "CreateSymbol": "CreateSymbol"
        }
      },
      "identifier": "sketchynotes-identifier"
    },
    {
      "name": "Add Note",
      "identifier": "addnote-identifier",
      "shortcut": "cmd option a",
      "script": "addnote.js",
      "handlers": {
        "run": "default",
        "actions": {
          "InsertSymbol": "CreateSymbol"
        }
      }
    }
  ],
  "menu": {
    "title": "Sketchy Notes",
    "items": [
      "sketchynotes-identifier",
      "addnote-identifier"
    ]
  },
  "version": "0.1.0",
  "name": "sketchynotes",
  "identifier": "sketchynotes",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com//master/.appcast.xml"
}